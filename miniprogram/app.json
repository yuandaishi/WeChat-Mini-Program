{
  "usingComponents": {
    "van-image": "@vant/weapp/image/index",
    "van-button": "@vant/weapp/button/index",
    "van-loading": "@vant/weapp/loading/index"
  },
  "entryPagePath": "pages/index/index",
  "pages": [
    "pages/index/index",
    "pages/cate/index",
    "pages/cart/index",
    "pages/user/index",
    "pages/detail/index",
    "pages/list/index"
  ],
  "window": {
    "navigationBarTextStyle": "black",
    "?navigationStyle": "custom",
    "navigationBarTitleText": "cocktail",
    "navigationBarBackgroundColor": "#ffffff",
    "enablePullDownRefresh": true,
    "?backgroundColor": "#bababa",
    "backgroundTextStyle": "dark"
  },
  "tabBar": {
    "selectedColor": "#1b9adc",
    "color": "#666",
    "backgroundColor": "",
    "borderStyle": "black",
    "position": "bottom",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/index/index",
        "iconPath": "/assets/icons/index.png",
        "selectedIconPath": "/assets/icons/index-active.png"
      },
      {
        "text": "分类",
        "pagePath": "pages/cate/index",
        "iconPath": "/assets/icons/cate.png",
        "selectedIconPath": "/assets/icons/cate-active.png"
      },
      {
        "text": "购物车",
        "pagePath": "pages/cart/index",
        "iconPath": "/assets/icons/cart.png",
        "selectedIconPath": "/assets/icons/cart-active.png"
      },
      {
        "text": "我的",
        "pagePath": "pages/user/index",
        "iconPath": "/assets/icons/my.png",
        "selectedIconPath": "/assets/icons/my-active.png"
      }
    ]
  },
  "style": "v2",
  "?renderer": "skyline",
  "?rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "tagNameStyleIsolation": "legacy",
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "?componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "subPackages": [
    {
      "root": "modules/goodModule",
      "name": "goodModule",
      "pages": [
        "pages/list/list",
        "pages/announcement/index"
      ]
    }
  ],
  "resolveAlias": {
    "@utils/*": "utils/*"
  }
}