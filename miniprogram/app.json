{
  "usingComponents": {
    "custom-checkbox": "/components/custom-checkbox/custom-checkbox",
    "custom01": "/components/custom01/custom01",
    "van-image": "@vant/weapp/image/index",
    "van-button": "@vant/weapp/button/index"
  },
  "entryPagePath": "pages/index/index",
  "pages": [
    "pages/index/index",
    "pages/hl/index",
    "pages/jzg/index",
    "pages/padan/index"
  ],
  "window": {
    "navigationBarTextStyle": "black",
    "?navigationStyle": "custom",
    "navigationBarTitleText": "袁代事的小程序",
    "navigationBarBackgroundColor": "#1b9adc",
    "enablePullDownRefresh": true,
    "?backgroundColor": "#bababa",
    "backgroundTextStyle": "dark"
  },
  "tabBar": {
    "selectedColor": "#1b9adc",
    "color": "#666",
    "backgroundColor": "",
    "borderStyle": "black",
    "position": "bottom",
    "list": [
      {
        "text": "成都",
        "pagePath": "pages/index/index",
        "iconPath": "/assets/icons/chengdu1.png",
        "selectedIconPath": "/assets/icons/chengdu2.png"
      },
      {
        "text": "九寨沟",
        "pagePath": "pages/jzg/index",
        "iconPath": "/assets/icons/hubo1.png",
        "selectedIconPath": "/assets/icons/hubo2.png"
      },
      {
        "text": "黄龙",
        "pagePath": "pages/hl/index",
        "iconPath": "/assets/icons/senlinheliu.png",
        "selectedIconPath": "/assets/icons/senlinheliu2.png"
      },
      {
        "text": "熊猫",
        "pagePath": "pages/padan/index",
        "iconPath": "/assets/icons/padan1.png",
        "selectedIconPath": "/assets/icons/padan2.png"
      }
    ]
  },
  "style": "v2",
  "?renderer": "skyline",
  "?rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "tagNameStyleIsolation": "legacy",
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "?componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "subPackages": [
    {
      "root": "modules/goodModule",
      "name": "goodModule",
      "pages": [
        "pages/list/list",
        "pages/announcement/index"
      ]
    }
  ]
}